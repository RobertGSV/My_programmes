#include <stdio.h>

int numbers[5];
int highest = 0;

int main(void)
{
printf("Enter 5 integers\n");
    int i=0;
    for(i=0; i < 4; i++)
    {
    scanf("%d\n", &numbers[i]);
    if (numbers[i] > highest)
    {
          highest = numbers[i];
    }
    }
    printf("The highest number is %d\n\n", highest);
  return 0;
}
