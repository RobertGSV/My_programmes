#include <stdio.h>

// find the highest value in an array


int numbers[5];

int main(void)
{
    printf("Enter 5 integers, pressing enter after each one entered\n");

    int highest;

    for(int i=0; i < 5; i++)
    {
    scanf("%d", &numbers[i]);
    }
    printf("You entered: \n");

    for (int j=0; j < 5; j++)
    {
    printf("%i, ", numbers[j]);
    }
    printf("\n");

    highest = numbers[0];

    for (int k=0; k < 5; k++)
    {
    if ((numbers[k] > highest))
    {
          highest = numbers[k];
    }

    }
    printf("The highest number is %i\n", highest);

}

