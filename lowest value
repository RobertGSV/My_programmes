#include <stdio.h>

// find the lowest value in an array


int numbers[5];

int main(void)
{
    printf("Enter 5 integers, pressing enter after each one entered\n");

    int lowest;

    for(int i=0; i < 5; i++)
    {
    scanf("%d", &numbers[i]);
    }
    printf("You entered: \n");

    for (int j=0; j < 5; j++)
    {
    printf("%i, ", numbers[j]);
    }
    printf("\n");


    for (int k=0; k < 5; k++)
    {
    if ((numbers[k] < lowest))
    {
          lowest = numbers[k];
    }

    }
    printf("The lowest number is %i\n", lowest);

}

